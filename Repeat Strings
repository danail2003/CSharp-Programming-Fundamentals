using System;
using System.Linq;

namespace Repeat_Strings
{
    class Program
    {
        static void Main()
        {
            string[] words = Console.ReadLine().Split().ToArray();

            for (int i = 0; i < words.Length; i++)
            {
                string word = words[i];

                for (int j = 0; j < word.Length; j++)
                {
                    Console.Write(word);
                }
            }
        }
    }
}
